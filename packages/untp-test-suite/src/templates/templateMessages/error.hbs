{
  "credentialType": "{{type}}",
  "version": "{{version}}",
  "path": "{{dataPath}}",
  "result": "FAIL",

  {{#if warnings.length}}
  "warnings": [
    {{#each warnings}}
      {
        "fieldName": "{{params.additionalProperty}}",
        "message": "This schema {{message}}"
      }
      {{#unless @last}},{{/unless}}
    {{/each}}
  ],
  {{/if}}

  "errors": [
    {{#each errors}}
      {
        "fieldName": "{{instancePath}}",
        "errorType": "{{keyword}}",
        {{#if params.allowedValues}}
        "allowedValues": {{{jsonStringify params.allowedValues}}},
        {{/if}}
        "message": "{{instancePath}} field {{message}}"
      }
      {{#unless @last}},{{/unless}}
    {{/each}}
  ]
}